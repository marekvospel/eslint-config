{
  "name": "@vospel/eslint-config-svelte",
  "version": "1.6.0",
  "license": "MIT",
  "description": "Marek Vospěl's svelte eslint configuration",
  "author": "Marek Vospěl",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marekvospel/eslint-config.git"
