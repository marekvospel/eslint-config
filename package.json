{
  "name": "eslint-config-vospel",
  "version": "1.2.2",
  "license": "MIT",
  "description": "Marek VospÄ›l's eslint configurations",
  "author": "Marek VospÄ›l",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json",
    "./configs/*": "./configs/*",
    "./base": "./configs/.eslintrc.base.js",
    "./typescript": "./configs/.eslintrc.typescript.js"
  },
  "repository": "https://github.com/marekvospel/eslint-config",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 5",
    "@typescript-eslint/parser": ">= 5",
    "eslint": ">= 8"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js . --ignore-pattern ./node_modules",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
